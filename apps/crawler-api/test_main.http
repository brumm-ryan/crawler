# User endpoints

# Create a user
POST http://127.0.0.1:8000/users/
Content-Type: application/json

{
  "username": "testuser",
  "email": "test@example.com",
  "full_name": "Test User"
}

###

# Get all users
GET http://127.0.0.1:8000/users/
Accept: application/json

###

# Get a specific user (replace 1 with an actual user ID after creating a user)
GET http://127.0.0.1:8000/users/1
Accept: application/json

###

# Item endpoints

# Create an item
POST http://127.0.0.1:8000/items/
Content-Type: application/json

{
  "name": "Test Item",
  "description": "This is a test item",
  "price": 19.99
}

###

# Get all items
GET http://127.0.0.1:8000/items/
Accept: application/json

###

# Get a specific item (replace 1 with an actual item ID after creating an item)
GET http://127.0.0.1:8000/items/1
Accept: application/json

